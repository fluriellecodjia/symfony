 {% extends "base.html.twig" %} 
 {% block title programme.title %}
 {% block body %}
   <div class="jumbotron">
     <div class="container mt-4">
       <div class="row">
         <div class="col-md-8">
         </div>
          <div class="col-md-4">
            <h1>{{programme.title}} </h1>
                 <a href="#" class="btn btn-primary">Contacter l'Eglise </a>
         </div>
     </div>
 </div>
       </div>
        <div class="container mt-4">
      
       <P>{{programme.description | nl2br}} </P>
     <div class="row">
          <div class="col-md-8">
          <h2>Caractéristiques </h2>
            <table class = "table table-striped">
          <tr>
             <td>Nom</td>
                <td>{{programme.nom}}</td>
          </tr>
           <tr>
             <td>Date </td>
                <td>{{programme.MomentAt  |date("m/d/y")}}</td>
          </tr>
            </table>
          </div>
         <div class="col-md-4">
             <h2>Spécificité </h2>
          </div>
       </div>
    </div>
       </div>
 {% endblock %}

