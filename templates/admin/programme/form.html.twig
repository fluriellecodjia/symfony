{{ form_start(form) }}
<div class="row">
     <div class="col-md-6">{{ form_row(form.title)}}</div>
     <div class="col-md-6">{{ form_row(form.description)}}</div>
    </div>
     <div class="row"> 
           <div class="col-md-5">{{ form_row(form.nom)}}</div>
             <div class="col-md-7">{{ form_row(form.moment_at )}}</div>
    </div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{button|default('Enregistrer')}} </button>
{{ form_end(form) }}
         

